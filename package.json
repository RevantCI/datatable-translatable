  "dependencies": {
    "axios": "0.18.1",
    "axios-cache-adapter": "^2.2.1",
    "deep-freeze": "^0.0.1",
    "jszip": "^3.2.0",
    "localforage": "^1.7.3",
    "markdown-translatable": "1.0.2-rc.2",
    "mui-datatables": "3.3.1",
    "use-deep-compare-effect": "^1.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@types/jest": "24.0.22",
    "coveralls": "^3.0.9",
    "fs-extra": "4.0.3",
    "react": "^16.8.6",
    "react-docgen": "4.1.1",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "react-styleguidist": "^9.1.11",
    "react-test-renderer": "^16.12.0",
    "webpack": "^4.0.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/core/**.{js,jsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
